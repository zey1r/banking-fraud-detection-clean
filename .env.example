# Banking Fraud Detection API - Environment Configuration
# Copy this file to .env and fill in your values

# Security
SECRET_KEY=your-super-secret-key-32-characters-long
JWT_SECRET_KEY=your-jwt-secret-key-32-characters-long

# Database
DATABASE_URL=sqlite:///./fraud_detection.db
# For PostgreSQL: postgresql://user:password@localhost:5432/fraud_db

# ML Model
MODEL_PATH=./models/fraud_model.joblib
MODEL_THRESHOLD=0.5

# Monitoring
ENABLE_MONITORING=true
LOG_LEVEL=INFO
